<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Web verification - suite">
  <test name="Web verification">
    <classes>
	      <class name="mslt.verification.Web_TV_Guide"/>
	      <class name="mslt.verification.Web_Signup_Login"/>
	      <class name="mslt.verification.Web_LikeDislike"/>
	      <class name="mslt.verification.Web_Share_Twitter"/>
	      <class name="mslt.verification.Web_Share_Facebook"/>
	      <class name="mslt.verification.Web_Activity"/>	      
	      <class name="mslt.verification.Web_ChannelConfiguration"/>	      
<!-- 	      <exclude name="mslt.verification.BroadcastValidateUtil"></exclude> -->
    </classes>
  </test> 
  
  <test name="Verify All info in Pelis">
    <parameter name="tag" value="Pelis"/> 
    <classes>
	      <class name="mslt.verification.Web_TV_Guide_BroadcastInfo"/>
    </classes>
  </test>
  <test name="Verify All info in Series">
    <parameter name="tag" value="Series"/> 
    <classes>
	      <class name="mslt.verification.Web_TV_Guide_BroadcastInfo"/>
    </classes>
  </test> 
  <test name="Verify All info in Deportes">
    <parameter name="tag" value="Deportes"/> 
    <classes>
	      <class name="mslt.verification.Web_TV_Guide_BroadcastInfo"/>
    </classes>
  </test>
  <test name="Verify All info in Ninos">
    <parameter name="tag" value="Ni"/> 
    <classes>
	      <class name="mslt.verification.Web_TV_Guide_BroadcastInfo"/>
    </classes>
  </test>
  <test name="Verify All info in Todos">
    <parameter name="tag" value="Todos"/> 
    <classes>
	      <class name="mslt.verification.Web_TV_Guide_BroadcastInfo"/>
    </classes>
  </test>
</suite> 